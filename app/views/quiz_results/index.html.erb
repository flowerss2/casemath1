<div class="page-header">
  <h1>
    Quiz results
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Quiz</th>
        <th>Score</th>
        <th>Actions</th>
      </tr>

      <% @quiz_results.each do |quiz_result| %>
      <tr>
        <td><%= quiz_result.user_id %></td>
        <td><%= quiz_result.quiz_id %></td>
        <td><%= quiz_result.score %></td>
        <td>
          <a href="/quiz_results/<%= quiz_result.id %>" class="btn btn-primary">Show</a>
          <a href="/quiz_results/<%= quiz_result.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_quiz_result/<%= quiz_result.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
